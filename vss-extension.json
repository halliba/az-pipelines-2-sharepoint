{
    "manifestVersion": 1,
    "id": "az-pipelines-2-sharepoint",
    "name": "Upload files to SharePoint Online",
    "version": "0.3.0",
    "publisher": "halliba",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Upload files to SharePoint Online",
    "categories": [
        "Azure Pipelines"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "icons": {
        "default": "images/icon_128.png"
    },
    "files": [
        {
            "path": "buildAndReleaseTask/dist/"
        },
        {
            "path": "buildAndReleaseTask/node_modules/"
        },
        {
            "path": "buildAndReleaseTask/task.json"
        },
        {
            "path": "buildAndReleaseTask/package.json"
        },
        {
            "path": "buildAndReleaseTask/yarn.lock"
        },
        {
            "path": "buildAndReleaseTask/icon.png"
        },
        {
            "path": "images/icon_128.png",
            "addressable": true
        }
    ],
    "contributions": [
        {
            "id": "az-pipelines-2-sharepoint",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "tags": [
        "SharePoint"
    ],
    "links": {
        "repository": {
            "uri": "https://github.com/halliba/az-pipelines-2-sharepoint"
        },
        "issues": {
            "uri": "https://github.com/halliba/az-pipelines-2-sharepoint/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/halliba/az-pipelines-2-sharepoint"
    }
}